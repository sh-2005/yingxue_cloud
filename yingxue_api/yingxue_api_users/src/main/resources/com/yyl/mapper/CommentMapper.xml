<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yyl.dao.ApiCommentDao">

    <insert id="insert" parameterType="com.yyl.entity.Comment">
        insert  into comment(uid,video_id,content,created_at,updated_at,deleted_at)
        values  (#{uid},#{videoId},#{content},#{createdAt},#{updatedAt},#{deletedAt})
    </insert>
</mapper>